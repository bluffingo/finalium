{% if is_chaziz_sb and not is_fulptube %}
    {% if current_theme == 'dark' %}
        {% set logo = '/assets/sb_branding/finalium/logo-wordmark-dark.png' %}
    {% else %}
        {% set logo = '/assets/sb_branding/finalium/logo-wordmark.png' %}
    {% endif %}
{% else %}
    {% set logo_suffix = current_theme == "dark" ? '-dark' : '' %}
    {% set logo_extension = website_branding.is_vector ? '.svg' : '.png' %}
    {% set logo_type = website_branding.use_wordmark ? 'logo-wordmark' : 'logo' %}

    {% set logo = website_branding.assets_location ~ '/' ~ logo_type ~ logo_suffix ~ logo_extension %}
{% endif %}

<!DOCTYPE html>
<html lang="en-US" class="{% if pin_guide %}show-guide guide-pinned{% endif %} {% if current_theme == "hitchhiker" %}hitchhiker{% endif %}">
<head>
    <meta charset="utf-8">
    <title>{% block title %}{% endblock %} - {{ website_branding.name }}</title>
    {% block head %}{% endblock %}
    <link rel="manifest" href="{{ website_branding.assets_location }}/icons/site.webmanifest">
    <link rel="shortcut icon" href="{{ website_branding.assets_location }}/icons/favicon.ico">
    <link rel="apple-touch-icon" sizes="180x180" href="{{ website_branding.assets_location }}/icons/apple-touch-icon.png">
    <link rel="icon" type="image/png" sizes="32x32" href="{{ website_branding.assets_location }}/icons/favicon-32x32.png">
    <link rel="icon" type="image/png" sizes="16x16" href="{{ website_branding.assets_location }}/icons/favicon-16x16.png">
    <meta name="apple-mobile-web-app-title" content="{{ website_branding.name }}">
    <meta name="application-name" content="{{ website_branding.name }}">
    <meta name="theme-color" content="#ffffff">
    <link rel="stylesheet" href="/assets/css/finalium_{{ current_theme }}.css?r={{ get_css_file_date() }}" type="text/css">
    <script src="/assets/skin/finalium/common.js?r={{ get_css_file_date() }}"></script>
    {% block script %}{% endblock %}
    <script>
        subscribe_string = '{{ localize('follow') }}';
        unsubscribe_string = '{{ localize('unfollow') }}';
        opensb_version = '{{ opensb_version.name }} {{ opensb_version.string }}';
    </script>
    <script src="/assets/common.js?r={{ get_css_file_date() }}"></script>
    <style>
        {% block customcss %}{% endblock %}
    </style>
</head>
<body id="body">
{% if is_debug %}
    {% include('components/debug_dialog.twig') %}
    <a id="debug-button">{{ localize('debug') }} @{{ opensb_version.hash }}</a>
{% endif %}
{% if show_warning_banner %}
    <div class="banner banner-info full-screen">
        <div class="banner-icon">
            {{ icon("asterisk", 16) }}
        </div>
        <div>
            {{ warning_banner_text | raw }}
        </div>
    </div>
{% endif %}
{% include "components/_guide.twig" %}
{% include "components/_header.twig" %}
{% block precontent %}{% endblock %}
<div class="container {{ page_name }}" id="page">
    <div id="content">
        {% if session.notif_message %}
            <div class="banner banner-{{ session.notif_color }}">
            {{ session.notif_message }}
            </div>
            {{ remove_notification() }}
        {% endif %}
        {% block content %}{% endblock %}
    </div>
</div>
{% include "components/_footer.twig" %}
</body>
</html>